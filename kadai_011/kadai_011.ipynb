      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文での主力\n",
        "for planet in array:\n",
        "  print(planet)\n",
        "\n",
        "# while文での出力\n",
        "# リストの中の個数を変数numに代入\n",
        "num = len(array)\n",
        "\n",
        "# while用のカウンタ変数を設定\n",
        "i = 0\n",
        "\n",
        "while i < num:\n",
        "  print(array[i])\n",
        "  i = i + 1"
      ]
